[
  "START + (' ' || '\n' || '\t' || '\r') = START",
  "START + '{' = ACCEPT_LBRACE",
  "START + '}' = ACCEPT_RBRACE",
  "START + '[' = ACCEPT_LBRACKET",
  "START + ']' = ACCEPT_RBRACKET",
  "START + ':' = ACCEPT_COLON",
  "START + ',' = ACCEPT_COMMA",
  "START + 'n' = NULL_AWAIT_U",
  "START + '\"' = STR_AWAIT_CHAR",

  "NULL_AWAIT_U + 'u' = NULL_AWAIT_L1",
  "NULL_AWAIT_L1 + 'l' = NULL_AWAIT_L2",
  "NULL_AWAIT_L2 + 'l' = ACCEPT_NULL",

  "STR_AWAIT_CHAR + !('\"' || '\\') = STR_AWAIT_CHAR",
  "STR_AWAIT_CHAR + '\"' = ACCEPT_STRING"
]