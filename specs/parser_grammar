GRAMMAR

ROOT -> { SECTIONS } $
SECTIONS -> epsilon
SECTIONS -> SECTION PSECTION
PSECTION -> epsilon
PSECTION -> , SECTION PSECTION
SECTION -> string : [ ITEMS ]
ITEMS -> epsilon
ITEMS -> ITEM PITEM
ITEM -> string
ITEM -> { PROP PPROP }
PITEM -> epsilon
PITEM -> , ITEM PITEM
PROP -> string : PROPVAL
PROPVAL -> string
PROPVAL -> STRINGLIST
PROPVAL -> STRINGDICT
PPROP -> epsilon
PPROP -> , PROP PPROP
STRINGLIST -> [ STRINGS ]
STRINGS -> epsilon
STRINGS -> string PSTRING
PSTRING -> epsilon
PSTRING -> , string PSTRING
STRINGDICT -> { STRINGPAIRS }
STRINGPAIRS -> epsilon
STRINGPAIRS -> STRINGPAIR PSTRINGPAIR
STRINGPAIR -> string : string
PSTRINGPAIR -> epsilon
PSTRINGPAIR -> , STRINGPAIR PSTRINGPAIR

EPSILONS

NULLIBLES = SECTIONS PSECTION ITEMS PITEM PPROP STRINGS PSTRING STRINGPAIRS PSTRINGPAIR

FIRST SETS

FIRST[ROOT] = {
FIRST[SECTIONS] = string
FIRST[SECTION] = string
FIRST[PSECTION] = ,
FIRST[ITEMS] = { string
FIRST[ITEM] = { string
FIRST[PITEM] = ,
FIRST[PROP] = string
FIRST[PPROP] = ,
FIRST[PROPVAL] = { string [
FIRST[STRINGLIST] = [
FIRST[STRINGDICT] = {
FIRST[STRINGS] = string
FIRST[PSTRING] = ,
FIRST[STRINGPAIRS] = string
FIRST[STRINGPAIR] = string
FIRST[PSTRINGPAIR] = ,

FOLLOW SETS

FOLLOW[ROOT] =
FOLLOW[SECTIONS] = }
FOLLOW[SECTION] = } ,
FOLLOW[PSECTION] = }
FOLLOW[ITEMS] = ]
FOLLOW[ITEM] = , ]
FOLLOW[PITEM] = ]
FOLLOW[PROP] = } ,
FOLLOW[PPROP] = }
FOLLOW[PROPVAL] = } ,
FOLLOW[STRINGLIST] = } ,
FOLLOW[STRINGDICT] = } ,
FOLLOW[STRINGS] = ]
FOLLOW[PSTRING] = ]
FOLLOW[STRINGPAIRS] = }
FOLLOW[STRINGPAIR] = } ,
FOLLOW[PSTRINGPAIR] = }