GRAMMAR

ROOT -> { SECTIONS } $
SECTIONS -> epsilon
SECTIONS -> SECTION PSECTION
PSECTION -> epsilon
PSECTION -> , SECTION PSECTION
SECTION -> string : [ ITEMS ]
ITEMS -> epsilon
ITEMS -> ITEM PITEM
ITEM -> string
ITEM -> { PROP PPROP }
PITEM -> epsilon
PITEM -> , ITEM PITEM
PROP -> string : PROPVAL
PROPVAL -> string
PROPVAL -> STRINGLIST
PROPVAL -> STRINGDICT
PPROP -> epsilon
PPROP -> , PROP PPROP
STRINGLIST -> [ STRINGPTS ]
STRINGPTS -> epsilon
STRINGPTS -> STRINGPT PSTRINGPT
STRINGPT -> string
PSTRINGPT -> epsilon
PSTRINGPT -> , string PSTRINGPT
STRINGDICT -> { STRINGPAIRS }
STRINGPAIRS -> epsilon
STRINGPAIRS -> STRINGPAIR PSTRINGPAIR
STRINGPAIR -> string : string
PSTRINGPAIR -> epsilon
PSTRINGPAIR -> , STRINGPAIR PSTRINGPAIR

EPSILONS

NULLIBLES = SECTIONS PSECTION ITEMS PITEM PPROP STRINGPTS PSTRINGPT STRINGPAIRS PSTRINGPAIR

FIRST SETS

FIRST[ROOT] = {
FIRST[SECTIONS] = string
FIRST[SECTION] = string
FIRST[PSECTION] = ,
FIRST[ITEMS] = { string
FIRST[ITEM] = { string
FIRST[PITEM] = ,
FIRST[PROP] = string
FIRST[PPROP] = ,
FIRST[PROPVAL] = { string [
FIRST[STRINGLIST] = [
FIRST[STRINGDICT] = {
FIRST[STRINGPTS] = string
FIRST[STRINGPT] = string
FIRST[PSTRINGPT] = ,
FIRST[STRINGPAIRS] = string
FIRST[STRINGPAIR] = string
FIRST[PSTRINGPAIR] = ,

FOLLOW SETS

FOLLOW[ROOT] =
FOLLOW[SECTIONS] = }
FOLLOW[SECTION] = } ,
